<!--URDF Launch -->
<launch>
    <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find gvrbot)/models/worlds/gvrbot.world"/> 
        <arg name="paused" value="true"/>
        <arg name="use_sim_time" value="true"/>
        <arg name="gui" value="true"/>
        <arg name="debug" value="false"/>
    </include>

    <!-- Convert an xacro and put on parameter server     --> 
    <include file="$(find gvrbot)/launch/description.launch"/>
    <include file="$(find gvrbot)/launch/control.launch"/>

    <!-- Spawn a robot into Gazebo -->  
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-z 0.25 -y -1.25 -param robot_description -urdf -model gvr01" />		
    <node pkg="interactive_marker_twist_server" type="marker_server" name="twist_marker_server"/>
