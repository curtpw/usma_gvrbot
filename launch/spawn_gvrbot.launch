<!--URDF Launch -->
<launch>
 
    <!-- Convert an xacro and put on parameter server  -->
	<param name= "robot_description07" command="$(find xacro)/xacro '$(find gvrbot)/models/robots/gvrbot.urdf.xacro'"/>

    <!-- Spawn a robot into Gazebo -->
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-z 2 -y -0.95 -param robot_description07 -urdf -model gvr07" />		
    
    <!-- <include file="$(find gvrbot)/launch/xbox360_teleop.launch"/>   -->

</launch>
